{
  "name": "vscode-blend-preview",
  "displayName": "Blend Preview",
  "description": "A Visual Studio Code extension for previewing .blend files.",
  "version": "0.1.0",
  "publisher": "your-name",
  "license": "MIT",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onLanguage:blend",
    "onCommand:blendPreview.open",
    "onCustomEditor:blendPreview.blendEditor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Blend Preview",
      "properties": {
        "blendPreview.gallery.thumbnailHeight": {
          "type": "number",
          "default": 140,
          "minimum": 60,
          "maximum": 512,
          "description": "Height (in pixels) for image thumbnails in the gallery."
        },
        "blendPreview.gallery.columns": {
          "type": "number",
          "default": 0,
          "minimum": 0,
          "maximum": 8,
          "description": "Number of columns for the image gallery. 0 uses auto-fill responsive layout."
        }
      }
    },
    "languages": [
      {
        "id": "blend",
        "aliases": ["Blend"],
        "extensions": [".blend"]
      }
    ],
    "customEditors": [
      {
        "viewType": "blendPreview.blendEditor",
        "displayName": "Blend File Editor",
        "selector": [
          { "filenamePattern": "*.blend" }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "blendPreview.open",
        "title": "Open Blend Preview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^4.9.0",
    "@types/vscode": "^1.60.0",
    "@types/node": "^16.x",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satishgoda/vscode-blend-preview.git"
  }
}